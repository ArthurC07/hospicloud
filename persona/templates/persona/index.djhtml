{% extends "base.djhtml" %}
{% block content %}
<nav class="toolbox">
  <ul>
    {% load menubuilder %}{% submenu menu-persona %}
    {% for item in submenu_items %}
    <li>
      <a href="{{ item.url }}" title="{{ item.title|escape }}"{% if item.current %} class='current'{% endif %}>{{ item.title }}</a>
    </li>
    {% endfor %}
  </ul>
</nav>
{% include "snippets/persona_list.djhtml" with personas=personas %}
{% endblock %}
 